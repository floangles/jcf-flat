<div class="col-xs-offset-3 col-xs-6 text-center">
  <div class="text-center">
    <%= simple_form_for [@place, @booking] do |f| %>
      <div class="text-center">
        <%= f.error_notification %>

        <%= f.input :guests %>
        <%= f.input :checkin %>
        <%= f.input :checkout %>

        <%= f.button :submit %>

      </div>
    <% end %>
  </div>
</div>
